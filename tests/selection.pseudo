DECLARE msg : STRING
FOR x <- 1 TO 10
    IF x MOD 2 = 0 AND x MOD 3 = 0 THEN
        msg <- "FizzBuzz"
    ELSE IF x MOD 2 = 0 THEN
        msg <- "Fizz"
    ELSE IF x MOD 3 = 0 THEN
        msg <- "Buzz"
    ELSE
        msg <- "None"
    ENDIF
    OUTPUT msg
NEXT x

FOR y <- 1 TO 100
    CASE OF y
        1: PRINT 1
        2: PRINT 2
        3: PRINT 3
        4: PRINT 4
        5 TO 10:
        10: PRINT 10
        11 TO 95.0:
        OTHERWISE: PRINT y 
    ENDCASE
NEXT y
